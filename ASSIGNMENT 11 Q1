#include<iostream>
using namespace std;
bool check(int arr[],int n){
    const int MAX=100;
    bool myhash[MAX]={false};
    for(int i=0;i<n;i++)
    {
        if(myhash[arr[i]])
        return true;
        myhash[arr[i]]=true;
    }
        return false;
}
int main(){
    int arr[]={1,2,1,3};
    int n=sizeof(arr)/sizeof(arr[0]);
    if(check(arr,n)){
        cout<<"DUPLICATES EXIST "<<endl;
    }
    else{
        cout<<"DUPLICATES DO NOT EXIST "<<endl;
    }
    return 0;
}
